<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.birthdayBackup.infrastructure.mapper.tag.TagMapper">

    <!-- タグを登録 -->
    <insert id="insert" parameterType="com.example.birthdayBackup.domain.model.Tag">
        INSERT INTO
            tag(
                tag_name,
                created_at
            )
        VALUES(
            #{tagName},
            NOW()
            )
    </insert>

</mapper>